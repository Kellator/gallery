{
  "name": "gallery",
  "version": "0.0.1",
  "description": "Gallery App designed from Thinkful React Template",
  "main": "js/index.js",
  "scripts": {
    "start": "http-server build",
    "dev": "run-p watch serve",
    "test": "mocha --recursive --compilers js:babel-core/register",
    "build": "run-s build:*",
    "clean": "rimraf build",
    "serve": "npm start",
    "watch": "run-p watch:*",
    "mkdir": "mkdirp build",
    "clean:html": "rimraf build/index.html",
    "mkdir:html": "run-s mkdir",
    "build:html": "run-s clean:html mkdir:html _build:html",
    "_build:html": "cpr index.html build/index.html",
    "watch:html": "run-s build:html _watch:html",
    "_watch:html": "chokidar index.html -c \"npm run build:html\"",
    "clean:js": "rimraf build/js",
    "mkdir:js": "mkdirp build/js",
    "build:js": "run-s clean:js mkdir:js _build:js",
    "_build:js": "webpack",
    "watch:js": "run-s build:js _watch:js",
    "_watch:js": "webpack --watch",
    "clean:css": "rimraf build/css",
    "mkdir:css": "run-s mkdir",
    "build:css": "run-s clean:css mkdir:css _build:css",
    "_build:css": "cpr css build/css",
    "watch:css": "run-s build:css _watch:css",
    "_watch:css": "chokidar css/**/* -c \"npm run build:css\"",
    "mkdir:assets": "run-s mkdir",
    "clean:assets": "rimraf build/assets",
    "build:assets": "run-s clean:assets mkdir:assets _build:assets",
    "_build:assets": "cpr assets build/assets",
    "watch:assets": "run-s build:assets _watch:assets",
    "_watch:assets": "chokidar assets/**/* -c \"npm run build:assets\""
  },
  "author": "Kelly Miller <k.macleod21@gmail.com>",
  "license": "UNLICENSED",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.5.2",
    "chokidar": "^1.7.0",
    "chokidar-cli": "^1.2.0",
    "cpr": "^2.2.0",
    "enzyme": "^2.9.1",
    "http-server": "^0.9.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.4.2",
    "npm-run-all": "^3.1.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.2.3",
    "rimraf": "^2.6.1",
    "sinon": "^1.17.7",
    "sinon-chai": "^2.11.0",
    "webpack": "2.x"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "axios-debug": "0.0.4",
    "babel-preset-env": "^1.6.0",
    "babel-preset-node6": "^11.0.0",
    "bcrypt": "^1.0.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "cors": "^2.8.4",
    "express": "^4.15.3",
    "express-session": "^1.15.4",
    "history": "^4.6.3",
    "isomorphic-fetch": "^2.2.1",
    "loader-utils": "^1.1.0",
    "mongodb": "^2.2.29",
    "mongoose": "^4.11.1",
    "passport": "^0.3.2",
    "passport-http": "^0.3.0",
    "passport-local": "^1.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^4.4.8",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^4.0.8",
    "react-transition-group": "^1.2.0",
    "redux": "^3.7.1",
    "redux-form": "^6.8.0",
    "redux-thunk": "^2.2.0",
    "semantic-ui-react": "^0.68.5",
    "socket.io-client": "^2.0.3"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kellator/gallery.git"
  },
  "bugs": {
    "url": "https://github.com/Kellator/gallery/issues"
  },
  "homepage": "https://github.com/Kellator/gallery#readme"
}
